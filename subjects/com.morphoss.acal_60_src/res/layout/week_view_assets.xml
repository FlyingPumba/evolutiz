<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
    
   <TextView
   		android:id="@+id/WV_header_day_box"  
		android:layout_height="fill_parent" 
		android:layout_width="fill_parent"
		style="@style/WV_header_day_box"
		android:visibility="gone">
	</TextView>
	<TextView
   		android:id="@+id/WV_header_multi_day_box"  
		android:layout_height="fill_parent" 
		android:layout_width="fill_parent"
		style="@style/WV_header_multi_day_box"
		android:visibility="gone">
	</TextView>
	<TextView
   		android:id="@+id/WV_side_box"  
		android:layout_height="fill_parent" 
		android:layout_width="fill_parent"
		style="@style/WV_side_box"
		android:visibility="gone">
	</TextView>
	<TextView
   		android:id="@+id/WV_side_box_half"  
		android:layout_height="fill_parent" 
		android:layout_width="fill_parent"
		style="@style/WV_side_box_half"
		android:visibility="gone">
	</TextView>
	<TextView
   		android:id="@+id/WV_event_box"  
		android:layout_height="fill_parent" 
		android:layout_width="fill_parent"
		style="@style/WV_event_box"
		android:visibility="gone">
	</TextView>
</LinearLayout>
