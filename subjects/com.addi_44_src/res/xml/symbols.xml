<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:keyOutputText="!" android:keyLabel="!" android:keyEdgeFlags="left"/>
        <Key android:keyOutputText="\@" android:keyLabel="\@"/>
        <Key android:keyOutputText="#" android:keyLabel="#"/>
        <Key android:keyOutputText="$" android:keyLabel="$"/>
        <Key android:keyOutputText="%" android:keyLabel="%"/>
        <Key android:keyOutputText="^" android:keyLabel="^"/>
        <Key android:keyOutputText="&amp;" android:keyLabel="&amp;"/>
        <Key android:keyOutputText="*" android:keyLabel="*"/>
        <Key android:keyOutputText="(" android:keyLabel="("/>
        <Key android:keyOutputText=")" android:keyLabel=")" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:keyOutputText="~" android:keyLabel="~" android:keyEdgeFlags="left"/>
        <Key android:keyOutputText="`" android:keyLabel="`"/>
        <Key android:keyOutputText="-" android:keyLabel="-"/>
        <Key android:keyOutputText="_" android:keyLabel="_"/>
        <Key android:keyOutputText="+" android:keyLabel="+"/>
        <Key android:keyOutputText="=" android:keyLabel="="/>
        <Key android:keyOutputText="[" android:keyLabel="["/>
        <Key android:keyOutputText="]" android:keyLabel="]"/>
        <Key android:keyOutputText="{" android:keyLabel="{"/>
        <Key android:keyOutputText="}" android:keyLabel="}" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:keyOutputText="\\" android:keyLabel="\\" android:horizontalGap="5%p" android:keyEdgeFlags="left"/>
        <Key android:keyOutputText="|" android:keyLabel="|"/>
        <Key android:keyOutputText=";" android:keyLabel=";"/>
        <Key android:keyOutputText=":" android:keyLabel=":"/>
        <Key android:keyOutputText="&apos;" android:keyLabel="&apos;"/>
        <Key android:keyOutputText="&quot;" android:keyLabel="&quot;"/>
        <Key android:keyOutputText="&gt;" android:keyLabel="&gt;"/>
        <Key android:keyOutputText="&lt;" android:keyLabel="&lt;"/>
        <Key android:keyOutputText="," android:keyLabel="," android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:keyOutputText="." android:keyLabel="." android:horizontalGap="15%p"/>
        <Key android:keyOutputText="/" android:keyLabel="/"/>
        <Key android:keyOutputText="\?" android:keyLabel="\?"/>
        <Key android:keyOutputText="**" android:keyLabel="**"/>
        <Key android:keyOutputText="e" android:keyLabel="e"/>
        <Key android:keyOutputText="pi" android:keyLabel="pi"/>
        <Key android:keyOutputText="Inf" android:keyLabel="Inf"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="15%p" android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
    	<Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyWidth="20%p" android:isModifier="true" android:isSticky="false" android:keyEdgeFlags="left"/>
        <Key android:codes="-102" android:keyLabel="ABC" android:keyWidth="20%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="35%p" android:popupKeyboard="@xml/keyboard_space"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyWidth="25%p" android:keyEdgeFlags="right"/>
    </Row>
    
</Keyboard>
    