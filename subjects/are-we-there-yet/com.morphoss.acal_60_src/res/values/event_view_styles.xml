<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

<style name="event_view_label">
	<item name="android:textSize">16sp</item>
	<item name="android:paddingLeft">4sp</item> 
	<item name="android:textColor">#333366</item>
</style>

<style name="event_view_content">
	<item name="android:textSize">18sp</item>
	<item name="android:paddingLeft">10sp</item> 
	<item name="android:textColor">#000000</item>
</style>

<style name="event_view_note">
	<item name="android:textSize">14sp</item>
	<item name="android:paddingLeft">4sp</item> 
	<item name="android:textColor">#555577</item>
</style>

<style name="thin_button">
    <item name="android:textSize">14sp</item>
    <item name="android:textColor">#001060</item>
    <item name="android:textStyle">bold</item>
    <item name="android:background">@drawable/thin_button_normal</item>
    <item name="android:height">14sp</item>
    <item name="android:padding">1sp</item>
</style>
</resources>