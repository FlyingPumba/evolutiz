<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="YVTitleBar">
		<item name="android:textColor">#F0F0F0</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textSize">16sp</item>
		<item name="android:padding">0px</item>
		<item name="android:gravity">center_horizontal|top</item>
	</style>
	<style name="YVMonthDayColumnHeader">
		<item name="android:textSize">10sp</item>
		<item name="android:height">14sp</item>
		<item name="android:padding">0px</item>
		<item name="android:textColor">#001060</item>
		<item name="android:textStyle">bold</item>
		<item name="android:gravity">center_horizontal|top</item>
	</style>
	<style name="YVMonthDayInMonth">
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">#001060</item>
		<item name="android:textStyle">bold</item>
		<item name="android:background">#A0DDDDDD</item>
		<item name="android:padding">0px</item>
		<item name="android:height">16sp</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="YVDayShadowed">
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">#80AAAAAA</item>
		<item name="android:textStyle">normal</item>
		<item name="android:padding">0px</item>
		<item name="android:height">16sp</item>
		<item name="android:gravity">center</item>
		<item name="android:background">#80AAAAAA</item>
	</style>
</resources>