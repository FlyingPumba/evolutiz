<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="17" android:versionName="2.90" package="org.dnaq.dialer2">
      
        <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:clearTaskOnLaunch="true" android:label="@string/app_name" android:name=".DialerActivity" android:theme="@android:style/Theme.NoTitleBar">

            <intent-filter>
                <action android:name="com.android.phone.action.TOUCH_DIALER"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.TAB"/>
            </intent-filter>
            
            <intent-filter>
            	<action android:name="android.intent.action.CALL_BUTTON"/>
            	<category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:mimeType="vnd.android.cursor.dir/calls"/>
            </intent-filter>
            
			 <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            
            <intent-filter>
                   <action android:name="com.android.phone.action.RECENT_CALLS"/>
                   <category android:name="android.intent.category.DEFAULT"/>
                   <category android:name="android.intent.category.TAB"/>
            </intent-filter>
            
			<intent-filter>
				   <action android:name="android.intent.action.DIAL"/>
				   <category android:name="android.intent.category.DEFAULT"/>			
			</intent-filter>            
			
            <intent-filter> <!--  filter to match the default dial action -->
            	<action android:name="android.intent.action.VIEW"/>
            	<action android:name="android.intent.action.DIAL"/>
            	<category android:name="android.intent.category.DEFAULT"/>
            	<category android:name="android.intent.category.BROWSABLE"/>
            	<data android:scheme="tel"/>
            </intent-filter>
        </activity>
        
        <activity android:name=".ContactViewDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name=".SelectNumberDialog" android:theme="@android:style/Theme.Dialog"/>
       	<activity android:name=".SelectSpeedDialSlotDialog" android:theme="@android:style/Theme.Dialog"/>
       	<activity android:name=".DialerPreferenceActivity"/>
    <receiver android:name="org.dnaq.dialer2.EmmaInstrument.SMSInstrumentedReceiver">
<intent-filter>
<action android:name="edu.gatech.m3.emma.COLLECT_COVERAGE"/>
</intent-filter>
</receiver>
<activity android:label="EmmaInstrumentationActivity" android:name="org.dnaq.dialer2.EmmaInstrument.InstrumentedActivity"/>
</application>
    <uses-sdk android:minSdkVersion="7"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/> 
    <uses-permission android:name="android.permission.CALL_PRIVILEGED"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
<instrumentation android:handleProfiling="true" android:label="EmmaInstrumentation" android:name="org.dnaq.dialer2.EmmaInstrument.EmmaInstrumentation" android:targetPackage="org.dnaq.dialer2"/>
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
</manifest>
